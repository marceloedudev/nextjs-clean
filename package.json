{
    "name": "nextjs-clean",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "npx cross-env NEXT_PUBLIC_ENVIRONMENT=development next dev",
        "dev:memory": "npx cross-env NEXT_PUBLIC_ENVIRONMENT=test next dev",
        "prepare": "husky install",
        "build": "next build",
        "export": "next export",
        "deploy": "next build && next export",
        "start": "next start",
        "next:lint": "next lint",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "test": "npx cross-env NEXT_PUBLIC_ENVIRONMENT=test npx jest --passWithNoTests --no-cache --runInBand --bail",
        "test:unit": "npm run test -- --config ./jest.config.unit.js",
        "test:unit:watch": "npm run test:unit -- --watch",
        "test:coverage": "npm run test:unit -- --coverage",
        "test:contract": "npx cross-env NEXT_PUBLIC_ENVIRONMENT=development npm run test -- --config ./jest.config.contract.js",
        "lint-staged": "npx lint-staged",
        "postinstall": "husky install",
        "test:cypress": "npx cypress open",
        "test:cypress:run": "npx cypress run",
        "test:cypress:ci": "npx start-server-and-test 'npm run dev' http://localhost:3000 'npm run test:cypress:run'",
        "ntl": "npx ntl"
    },
    "dependencies": {
        "@emotion/css": "^11.9.0",
        "@emotion/react": "^11.9.0",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.8.1",
        "@rollbar/react": "^0.11.1",
        "axios": "^0.27.2",
        "babel-preset-next": "^1.4.0",
        "next": "12.1.6",
        "next-intercept-stdout": "^1.0.1",
        "nprogress": "^0.2.0",
        "polished": "^4.2.2",
        "react": "18.1.0",
        "react-cookie": "^4.1.1",
        "react-dom": "18.1.0",
        "react-hot-toast": "2.2.0",
        "react-icons": "^4.3.1",
        "recoil": "0.7.2",
        "rollbar": "^2.25.0",
        "safe-regex": "^2.1.1",
        "universal-cookie": "^4.0.4"
    },
    "devDependencies": {
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/runtime": "^7.18.3",
        "@badeball/cypress-cucumber-preprocessor": "^10.0.2",
        "@cypress/browserify-preprocessor": "^3.0.2",
        "@cypress/webpack-preprocessor": "^5.12.0",
        "@emotion/babel-preset-css-prop": "^11.2.0",
        "@jest/globals": "^28.1.1",
        "@pact-foundation/pact": "^9.17.3",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@types/jest": "^27.4.1",
        "@types/node": "17.0.31",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.3",
        "@typescript-eslint/eslint-plugin": "5.20.0",
        "@typescript-eslint/parser": "5.20.0",
        "babel-plugin-transform-goober": "^1.2.0",
        "cross-env": "^7.0.3",
        "cypress": "^9.6.1",
        "dotenv": "^16.0.1",
        "eslint": "^8.13.0",
        "eslint-config-next": "^12.1.5",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.4.0",
        "faker-br": "^0.4.1",
        "husky": "^7.0.2",
        "jest": "^27.0.4",
        "jest-cucumber": "^3.0.1",
        "jest-environment-jsdom": "^27.0.4",
        "lint-staged": "^12.4.0",
        "ntl": "^5.1.0",
        "prettier": "^2.6.2",
        "react-is": "^18.1.0",
        "start-server-and-test": "^1.14.0",
        "ts-jest": "^27.0.2",
        "ts-loader": "^9.3.0",
        "typescript": "^4.6.4",
        "webpack": "^5.72.0"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true,
        "stepDefinitions": [
            "./tests/e2e/cypress/integration/[filepath]/**/*.{js,ts}",
            "./tests/e2e/cypress/integration/[filepath].{js,ts}",
            "./tests/e2e/cypress/support/step_definitions/**/*.{js,ts}"
        ]
    }
}